  <!-- Attributes for the Machine_Learning_Algorithm classes.   -->
  
  <DD_Attribute>
    <name>algorithm_learning_style</name>
    <version_id>1.0</version_id>
    <local_identifier>algorithm_learning_style</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Minh Le</submitter_name>
    <definition>The algorithm_learning_style attribute describes the type of learning style adopted by the algorithm to model a problem.</definition>
    <DD_Value_Domain>
      <enumeration_flag>true</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
      <DD_Permissible_Value>
        <value>Supervised_Learning</value>
        <value_meaning>Supervised_Learning indicates that labeled data has been used to teach models to yield the desired output.</value_meaning>
      </DD_Permissible_Value>
      <DD_Permissible_Value>
        <value>Semisupervised_Learning</value>
        <value_meaning>Both labeled data and a large amount of unlabeled data have been used during training.</value_meaning>
      </DD_Permissible_Value>
      <DD_Permissible_Value>
        <value>Unsupervised_Learning</value>
        <value_meaning>The algorithm has not received any labeled data and has discovered patterns on its own from untagged input.</value_meaning>
      </DD_Permissible_Value>
    </DD_Value_Domain>
  </DD_Attribute>
  <DD_Attribute>
    <name>algorithm_type</name>
    <version_id>1.0</version_id>
    <local_identifier>algorithm_type</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Minh Le</submitter_name>
    <definition>The algorithm_type attribute describes the kind of algorithm used, such as a regression, neural network, tree, or etc.</definition>
    <DD_Value_Domain>
      <enumeration_flag>true</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
      <DD_Permissible_Value>
        <value>Convolutional_Neural_Network_Classifier</value>
        <value_meaning>The algorithm used for image analysis is a Convolutional Neural Network (CNN) Classifier.</value_meaning>
      </DD_Permissible_Value>
    </DD_Value_Domain>
  </DD_Attribute>
  <DD_Attribute>
    <name>algorithm_name</name>
    <version_id>1.0</version_id>
    <local_identifier>algorithm_name</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Minh Le</submitter_name>
    <definition>The algorithm_name attribute specifies the name of the algorithm used.</definition>
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
    </DD_Value_Domain>
  </DD_Attribute>
  
  <!-- Attributes for the Data_Set_Preparation classes.   -->
  
