  <!-- Attributes -->
  
  <!-- Attributes for the Image_Data_Set classes.   -->
  
  <DD_Attribute>
    <name>data_set_size</name>
    <version_id>1.0</version_id>
    <local_identifier>data_set_size</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Minh Le</submitter_name>
    <definition>The data_set_size attribute provides the number of
    images within the data set.</definition> 
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Real</value_data_type>
      <unit_of_measure_type>Units_of_None</unit_of_measure_type>
    </DD_Value_Domain>
  </DD_Attribute>
  <DD_Attribute>
    <name>data_set_version_id</name>
    <version_id>1.0</version_id>
    <local_identifier>data_set_version_id</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Minh Le</submitter_name>
    <definition>The data_set_version_id attribute specifies the data
    set version number.</definition> 
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
      <unit_of_measure_type>Units_of_None</unit_of_measure_type>
    </DD_Value_Domain>
  </DD_Attribute>
  
  <!-- Attributes for the Machine_Learning_Algorithm class.   -->
  
  <DD_Attribute>
    <name>algorithm_learning_style</name>
    <version_id>1.0</version_id>
    <local_identifier>algorithm_learning_style</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Minh Le</submitter_name>
    <definition>The algorithm_learning_style attribute describes the
    type of learning style adopted by the algorithm to model a
    problem.</definition> 
    <DD_Value_Domain>
      <enumeration_flag>true</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
      <DD_Permissible_Value>
        <value>Supervised_Learning</value>
        <value_meaning>Supervised_Learning indicates that labeled data
        has been used to teach models to yield the desired
        output.</value_meaning> 
      </DD_Permissible_Value>
      <DD_Permissible_Value>
        <value>Semisupervised_Learning</value>
        <value_meaning>Both labeled data and a large amount of
        unlabeled data have been used during training.</value_meaning> 
      </DD_Permissible_Value>
      <DD_Permissible_Value>
        <value>Unsupervised_Learning</value>
        <value_meaning>The algorithm has not received any labeled data
        and has discovered patterns on its own from untagged
        input.</value_meaning> 
      </DD_Permissible_Value>
    </DD_Value_Domain>
  </DD_Attribute>
  <DD_Attribute>
    <name>algorithm_type</name>
    <version_id>1.0</version_id>
    <local_identifier>algorithm_type</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Minh Le</submitter_name>
    <definition>The algorithm_type attribute describes the kind of
    algorithm used, such as a regression, neural network, tree, or
    etc.</definition> 
    <DD_Value_Domain>
      <enumeration_flag>true</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
      <DD_Permissible_Value>
        <value>Convolutional_Neural_Network_Classifier</value>
        <value_meaning>The algorithm used for image analysis is a
        Convolutional Neural Network (CNN) Classifier.</value_meaning> 
      </DD_Permissible_Value>
    </DD_Value_Domain>
  </DD_Attribute>
  <DD_Attribute>
    <name>algorithm_name</name>
    <version_id>1.0</version_id>
    <local_identifier>algorithm_name</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Minh Le</submitter_name>
    <definition>The algorithm_name attribute specifies the name of the
    algorithm used.</definition> 
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
    </DD_Value_Domain>
  </DD_Attribute>
  
  <!-- Attributes for the Trained_Machine_Learning_Model class.   -->

  <DD_Attribute>
    <name>trained_model_version_id</name>
    <version_id>1.0</version_id>
    <local_identifier>trained_model_version_id</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Kiri L. Wagstaff</submitter_name>
    <definition>The trained_model_version_id attribute specifies the
    trained model version number.</definition> 
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
      <unit_of_measure_type>Units_of_None</unit_of_measure_type>
    </DD_Value_Domain>
  </DD_Attribute>
  <DD_Attribute>
    <name>trained_model_name</name>
    <version_id>1.0</version_id>
    <local_identifier>trained_model_name</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Kiri L. Wagstaff</submitter_name>
    <definition>The trained_model_name attribute specifies the name of the
    model used.</definition> 
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
    </DD_Value_Domain>
  </DD_Attribute>
  <DD_Attribute>
    <name>trained_model_metric</name>
    <version_id>1.0</version_id>
    <local_identifier>trained_model_metric</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Kiri L. Wagstaff</submitter_name>
    <definition>The trained_model_metric attribute describes the
    metric that was used to evaluate the model's
    performance.</definition>   
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
    </DD_Value_Domain>
  </DD_Attribute>
  <DD_Attribute>
    <name>trained_model_performance</name>
    <version_id>1.0</version_id>
    <local_identifier>trained_model_performance</local_identifier>
    <nillable_flag>false</nillable_flag>
    <submitter_name>Kiri L. Wagstaff</submitter_name>
    <definition>The trained_model_performance attribute reports the
    score obtained by the model using
    trained_model_metric.</definition>    
    <DD_Value_Domain>
      <enumeration_flag>false</enumeration_flag>
      <value_data_type>ASCII_Real</value_data_type>
      <unit_of_measure_type>Units_of_None</unit_of_measure_type>
    </DD_Value_Domain>
  </DD_Attribute>

  <!-- Classes -->
  
  <!-- Image_Data_Set Classes -->
  <DD_Class>
    <name>Image_Data_Set</name>
    <version_id>1.0</version_id>
    <local_identifier>Image_Data_Set</local_identifier>
    <submitter_name>Minh Le</submitter_name>
    <definition>The Image_Data_Set class is the container for classes
    and attributes describing the provenance, size and processing
    details of data sets used by the machine learning model.</definition>
    <abstract_flag>true</abstract_flag>
    <element_flag>false</element_flag>
    <DD_Association>
      <identifier_reference>data_set_version_id</identifier_reference>
      <reference_type>attribute_of</reference_type>
      <minimum_occurrences>1</minimum_occurrences>
      <maximum_occurrences>1</maximum_occurrences>
    </DD_Association>
    <DD_Association>
      <identifier_reference>data_set_size</identifier_reference>
      <reference_type>attribute_of</reference_type>
      <minimum_occurrences>1</minimum_occurrences>
      <maximum_occurrences>1</maximum_occurrences>
    </DD_Association>
  </DD_Class>
  <DD_Class>
    <name>Training_Set</name>
    <version_id>1.0</version_id>
    <local_identifier>Training_Set</local_identifier>
    <submitter_name>Minh Le</submitter_name>
    <definition>The Training_Set class belongs to the Image_Data_Set
    class family and contains attributes that describe the provenance,
    size, and processing details for the machine learning model
    training set.</definition> 
    <element_flag>false</element_flag>
    <DD_Association>
      <identifier_reference>Image_Data_Set</identifier_reference>
      <reference_type>parent_of</reference_type>
      <minimum_occurrences>1</minimum_occurrences>
      <maximum_occurrences>1</maximum_occurrences>
    </DD_Association>
  </DD_Class>
  <DD_Class>
    <name>Validation_Set</name>
    <version_id>1.0</version_id>
